[Unit]
Description=arrowhead-serviceregistry-sql
After=network.target mysql.target

[Service]
WorkingDirectory=/etc/arrowhead/service-registry-sql
ExecStart=/usr/bin/java --add-modules java.xml.bind -jar /usr/share/arrowhead/systems/arrowhead-serviceregistry-sql-4.0.jar -d -daemon -tls
Type=simple
User=arrowhead
Group=arrowhead

[Install]
WantedBy=default.target
